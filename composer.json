{
    "name": "phpackage/autodns-php-sdk",
    "description": "InterNetX Domain Robot JSON API SDK",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Manuel Mendoza",
            "email": "alejobit@gmail.com"
        }
    ],
    "require": {
        "ext-json": "*",
        "rmccue/requests": "^1.7",
        "symfony/serializer": "^4.0",
        "symfony/property-access": "^4.0",
        "symfony/property-info": "^4.0"
    },
    "require-dev": {
        "phpstan/phpstan": "^0.11.8",
        "squizlabs/php_codesniffer": "^3.4",
        "phpunit/phpunit": "^8",
        "fzaninotto/faker": "^1.8"
    },
    "autoload": {
        "psr-4": {
            "Phpackage\\Autodns\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Phpackage\\Autodns\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "phpstan": "phpstan analyse -l 7 src/",
        "phpunit": "phpunit --bootstrap vendor/autoload.php --testdox tests/",
        "test": [
            "phpcs",
            "@phpstan",
            "@phpunit"
        ]
    }
}
